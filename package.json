{
  "name": "eventhub-xxl",
  "version": "2.0.5",
  "description": "Event Hub for event-based applications",
  "main": "./dist/eventhub.umd.js",
  "module": "./dist/eventhub.min.js",
  "es2015": "./src/eventhub.js",
  "tonicExampleFilename": "runkit.js",
  "scripts": {
    "build": "mkdir -p dist && ./node_modules/.bin/npm-run-all build:*",
    "build:es5": "./node_modules/.bin/babel ./src/eventhub.js -o dist/eventhub.js && ./node_modules/.bin/uglifyjs -o dist/eventhub.min.js dist/eventhub.js",
    "build:umd": "./node_modules/.bin/babel --module-id xxl --plugins transform-es2015-modules-umd ./src/eventhub.js -o ./dist/eventhub.umd.js && ./node_modules/.bin/uglifyjs -o dist/eventhub.umd.min.js dist/eventhub.umd.js",
    "lint": "./node_modules/.bin/eslint -c .eslintrc ./src/eventhub.js",
    "doc": "./node_modules/.bin/jsdoc src/eventhub.js -c jsdoc.json --destination docs && open docs/index.html",
    "prepublish": "yarn build",
    "test": "cross-env NODE_ENV=test nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scaljeri/eventhub-xxl.git"
  },
  "keywords": [
    "javascript",
    "eventhub"
  ],
  "author": "Lucas Calje <lucas@calje.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scaljeri/eventhub-xxl/issues"
  },
  "homepage": "https://github.com/scaljeri/eventhub-xxl",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "chai": "^4.0.2",
    "chai-arrays": "^2.0.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.0.1",
    "jsdoc": "^3.5.4",
    "jsdoc-escape-at": "^1.0.1",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.0.3",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.11.0",
    "uglify-js": "^3.0.28"
  },
  "dependencies": {}
}
