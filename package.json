{
  "name": "eventhub-xxl",
  "version": "2.0.0-rc.5",
  "description": "Event Hub for event-based applications",
  "main": "./src/eventhub.js",
  "scripts": {
    "build": "mkdir -p dist && ./node_modules/.bin/babel ./src/eventhub.js --out-file dist/eventhub.js && ./node_modules/.bin/uglifyjs dist/eventhub.js -o dist/eventhub.min.js",
    "lint": "./node_modules/.bin/eslint -c .eslintrc ./src/eventhub.js",
    "doc": "./node_modules/.bin/jsdoc src/eventhub.js -c jsdoc.json --destination docs && open docs/index.html",
    "prepublish": "npm run build",
    "test": "cross-env NODE_ENV=test nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scaljeri/eventhub-xxl.git"
  },
  "keywords": [
    "javascript",
    "eventhub"
  ],
  "author": "Lucas Calje <lucas@calje.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scaljeri/eventhub-xxl/issues"
  },
  "homepage": "https://github.com/scaljeri/eventhub-xxl",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "esdoc": "1.0.1",
    "esdoc-es7-plugin": "0.0.3",
    "isparta": "4.0.0",
    "mocha": "^3.4.2",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.11.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "chai-arrays": "^1.0.0",
    "cross-env": "^5.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.4.1",
    "jsdoc": "^3.5.4",
    "jsdoc-escape-at": "^1.0.1",
    "nyc": "^11.0.3",
    "uglify-js": "^3.0.28"
  }
}
